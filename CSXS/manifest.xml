<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest ExtensionBundleId="com.my.imagekit.widget" ExtensionBundleVersion="1.0.0" Version="10.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.my.imagekit.panel" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PHSP" Version="22.0" />
      <Host Name="PHXS" Version="22.0" />
      <Host Name="IDSN" Version="16.0" />
      <Host Name="AICY" Version="16.0" />
      <Host Name="ILST" Version="25.0" />
      <Host Name="PPRO" Version="14.4" />
      <!-- <Host Name="PRLD" Version="9" /> -->
      <Host Name="AEFT" Version="17.1.4" />
      <Host Name="FLPR" Version="21.0" />
      <Host Name="AUDT" Version="13.0.10" />
      <Host Name="DRWV" Version="21.0" />
      <Host Name="KBRD" Version="11.0" />
      <Host Name="RUSH" Version="1.5.29" />
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="10.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.my.imagekit.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <CEFCommandLine>
            <Parameter>--allow-file-access</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>ImageKit Panel</Menu>
          <Geometry>
            <Size>
              <Height>500</Height>
              <Width>350</Width>
            </Size>
          </Geometry>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>